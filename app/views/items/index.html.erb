<div class="container">
  <h1>List of items</h1>
  <% @items.each do |item| %>
  <ul>
    <li>
    <h2><%= link_to item.name, item_path(item) %></h2>
    <%= link_to 'edit', edit_item_path(item) %>
    <%= link_to 'delete', item_path(item), method: :delete %>
    </li>
  </ul>
    <% end %>
    <%= link_to 'add an item', new_item_path(@item) %>
</div>
